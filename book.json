{
  "title": "Site Title",
  "author": "Author Name",
  "authorHomepage": "http://www.example.com",
  "baiduStatisticsCode": "",
  "description": "site description",
  "language": "en-za",
  "copyright": "All Rights Reserved",
  "variables": {
    "homePage": "/"
  },
  "plugins": [
    "-lunr","-search","search-plus",
    "mermaid-v8",
    "terminal",
    "prism",
    "-highlight",
    "collapsible-chapters",
    "insert-logo",
    "prism-themes",
    "github-edit",
    "intopic-toc"
  ],
  "pluginsConfig": {
    "terminal": {
      "copyButtons": true,
      "fade": false,
      "style": "classic"
    },
    "insert-logo": {
      "url": "https://github.com/neilrussell6/gitbook-playground/blob/master/logo.png?raw=true",
      "style": "background: none;"
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 2
    },
    "github-edit": {
      "repo": "https://github.com/neilrussell6/gitbook-playground",
      "branch": "master"
    },
    "collapsible-chapters": {},
    "intopic-toc": {
      "label": "Contents"
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-a11y-dark.css"
      ],
      "lang": {
        "flow": "typescript"
      },
      "ignore": [
        "mermaid",
        "eval-js"
      ]
    }
  }
}
